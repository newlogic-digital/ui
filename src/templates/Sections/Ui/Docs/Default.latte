                                <input type="number" placeholder="" value="1" min="1" max="99" step="1">
                                <div class="ms-auto">
                                    <div>Kč</div>
                                </div>
                            </div>
                        {/capture}{$code|code('html', true)|noescape}
                        {capture $code}
