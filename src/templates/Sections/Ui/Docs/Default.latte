                                <div class="ms-auto">
                                    <svg data-action="click->ui-control#showDatepicker">
                                        <use href="#icon-calendar"></use>
                                    </svg>
                                </div>
                            </div>
                        {/capture}{$code|code('html', true)|noescape}
                        {capture $code}
                            <div class="ui-control">
                                <input type="date">
                                <label>Select date</label>
                            </div>
                        {/capture}{$code|code('html', true)|noescape}
                        {capture $code}
